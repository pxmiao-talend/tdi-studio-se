<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
	PARTITIONING="NONE"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Sybase</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:SYBASE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="20"
	  SHOW_IF="DB_VERSION == 'SYBSEIQ_12'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tSybaseConnection"
      NUM_ROW="20"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"
    />

	<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="23"
		SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
		<ITEMS DEFAULT="SYBSEIQ_12">
			<ITEM NAME="SYBSEIQ_12" VALUE="jconn3.jar" />
			<ITEM NAME="SYBSEIQ_15" VALUE="jodbc.jar" />
		</ITEMS>
	</PARAMETER>

	<PARAMETER NAME="DATA_SOURCE" FIELD="CLOSED_LIST" NUM_ROW="25"
		REPOSITORY_VALUE="DATA_SOURCE" SHOW_IF="(DB_VERSION == 'SYBSEIQ_15')">
		<ITEMS DEFAULT="DATA_SOURCE_NAME">
			<ITEM NAME="DATA_SOURCE_NAME" VALUE="DSN" />
			<ITEM NAME="FILE_DATA_SOURCE_NAME" VALUE="FILEDSN" />
		</ITEMS>
	</PARAMETER>
	<PARAMETER
      NAME="DATA_SOURCE_NAME"
      FIELD="TEXT"
      NUM_ROW="25"
      REPOSITORY_VALUE="DATA_SOURCE_NAME"
      REQUIRED="true"
      SHOW_IF="(DATA_SOURCE == 'DATA_SOURCE_NAME') AND (DB_VERSION == 'SYBSEIQ_15')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
       <PARAMETER NAME="FILE_DATA_SOURCE_NAME" FIELD="FILE"
       NUM_ROW="25" REQUIRED="true"
       REPOSITORY_VALUE="FILE_DATA_SOURCE_NAME"
       SHOW_IF="(DATA_SOURCE == 'FILE_DATA_SOURCE_NAME') AND (DB_VERSION == 'SYBSEIQ_15')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="true"
      REPOSITORY_VALUE="SERVER_NAME"
       SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION != 'SYBSEIQ_15')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

   <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="true"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION != 'SYBSEIQ_15')"
    >
      <DEFAULT>"2648"</DEFAULT>
   </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REQUIRED="true"
      REPOSITORY_VALUE="SID"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="DB_SCHEMA"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SCHEMA"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="50"
      REQUIRED="true"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="50"
      REQUIRED="true"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="60"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="60"
      NOT_SHOW_IF="PARALLELIZE == 'true'"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="70"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/sybaseIQ_bulk.txt"</DEFAULT>
    </PARAMETER>

    <PARAMETER
    	NAME="APPEND"
    	FIELD="CHECK"
    	NUM_ROW="80">

      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="90"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="100"
      SHOW="false"
    >
      <DEFAULT>sybase_id</DEFAULT>
    </PARAMETER>

 </PARAMETERS>

 <ADVANCED_PARAMETERS>

  	<PARAMETER
		NAME="PROPERTIES"
		FIELD="TEXT"
		NUM_ROW="4"
		REPOSITORY_VALUE="PROPERTIES_STRING"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

 	<PARAMETER
      NAME="FIELDS_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LINES_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCLOSED"
      FIELD="CHECK"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

	<PARAMETER
    	NAME="INCLUDEHEADER"
    	FIELD="CHECK"
    	NUM_ROW="9">

      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
       NAME="BLANK_AS_NULL"
       FIELD="CHECK"
       NUM_ROW="10"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
    	NAME="ENCODING"
    	FIELD="ENCODING_TYPE"
    	NUM_ROW="11"
    	REQUIRED="true" >

      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

 </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="tSOB" OUTPUT="tSBE">

      <TEMPLATE NAME="tSOB" COMPONENT="tSybaseOutputBulk">
        <LINK_TO NAME="tSBE" CTYPE="ROWS_END"/>
      </TEMPLATE>

      <TEMPLATE NAME="tSBE" COMPONENT="tSybaseIQBulkExec"/>
        <TEMPLATE_PARAM SOURCE="self.PROPERTY" TARGET="tSBE.PROPERTY"></TEMPLATE_PARAM>
		<TEMPLATE_PARAM SOURCE="self.DATA_SOURCE" TARGET="tSBE.DATA_SOURCE" />
		<TEMPLATE_PARAM SOURCE="self.DATA_SOURCE_NAME" TARGET="tSBE.DATA_SOURCE_NAME" />
		<TEMPLATE_PARAM SOURCE="self.FILE_DATA_SOURCE_NAME" TARGET="tSBE.FILE_DATA_SOURCE_NAME" />
        <TEMPLATE_PARAM SOURCE="self.USE_EXISTING_CONNECTION" TARGET="tSBE.USE_EXISTING_CONNECTION" />
        <TEMPLATE_PARAM SOURCE="self.CONNECTION" TARGET="tSBE.CONNECTION" />
        <TEMPLATE_PARAM SOURCE="self.HOST" TARGET="tSBE.HOST"></TEMPLATE_PARAM>
        <TEMPLATE_PARAM SOURCE="self.PORT" TARGET="tSBE.PORT" />
        <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="tSBE.DBNAME" />
        <TEMPLATE_PARAM SOURCE="self.DB_SCHEMA" TARGET="tSBE.DB_SCHEMA" />
	    <TEMPLATE_PARAM SOURCE="self.USER" TARGET="tSBE.USER" />
	    <TEMPLATE_PARAM SOURCE="self.PASS" TARGET="tSBE.PASS" />
	    <TEMPLATE_PARAM SOURCE="self.TABLE" TARGET="tSBE.TABLE" />
	    <TEMPLATE_PARAM SOURCE="self.TABLE_ACTION" TARGET="tSBE.TABLE_ACTION" />
	    <TEMPLATE_PARAM SOURCE="self.SCHEMA"   TARGET="tSBE.SCHEMA" />
    	<TEMPLATE_PARAM SOURCE="self.MAPPING" TARGET="tSBE.MAPPING" />
        <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tSBE.FILENAME" />
        <TEMPLATE_PARAM SOURCE="self.LINES_TERMINATED_BY" TARGET="tSBE.LINES_TERMINATED_BY" />
        <TEMPLATE_PARAM SOURCE="self.FIELDS_TERMINATED_BY" TARGET="tSBE.FIELDS_TERMINATED_BY" />
        <TEMPLATE_PARAM SOURCE="self.ENCLOSED" TARGET="tSBE.ENCLOSED" />
        <TEMPLATE_PARAM SOURCE="self.PROPERTIES" TARGET="tSBE.PROPERTIES" />
        <TEMPLATE_PARAM SOURCE="self.DB_VERSION" TARGET="tSBE.DB_VERSION" />
        <TEMPLATE_PARAM SOURCE="self.INCLUDEHEADER" TARGET="tSBE.INCLUDEHEADER" />
        <TEMPLATE_PARAM SOURCE="self.BLANK_AS_NULL" TARGET="tSBE.BLANK_AS_NULL" />

		<TEMPLATE_PARAM SOURCE="self.PROPERTY" TARGET="tSOB.PROPERTY"></TEMPLATE_PARAM>
		<TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tSOB.FILENAME" />
		<TEMPLATE_PARAM SOURCE="self.APPEND" TARGET="tSOB.APPEND" />
	    <TEMPLATE_PARAM SOURCE="self.SCHEMA"   TARGET="tSOB.SCHEMA" />
    	<TEMPLATE_PARAM SOURCE="self.MAPPING" TARGET="tSSOB.MAPPING" />
        <TEMPLATE_PARAM SOURCE="self.LINES_TERMINATED_BY" TARGET="tSOB.ROWSEPARATOR" />
        <TEMPLATE_PARAM SOURCE="self.FIELDS_TERMINATED_BY" TARGET="tSOB.FIELDSEPARATOR" />
        <TEMPLATE_PARAM SOURCE="self.INCLUDEHEADER" TARGET="tSOB.INCLUDEHEADER" />
        <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tSOB.ENCODING" />

    </TEMPLATES>
    <IMPORTS>
       <IMPORT NAME="Driver-Sybase3" MODULE="jconn3.jar" MVN="mvn:org.talend.libraries/jconn3/6.0.0"  REQUIRED="true" />
       <IMPORT NAME="Driver-Sybase" MODULE="jodbc.jar" MVN="mvn:org.talend.libraries/jodbc/6.0.0"  REQUIRED="false" />
    </IMPORTS>
  </CODEGENERATION>
  <RETURNS/>
</COMPONENT>
