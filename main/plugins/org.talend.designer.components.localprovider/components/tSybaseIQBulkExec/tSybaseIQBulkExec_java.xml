<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Sybase</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
        NAME="NOTE"
        FIELD="LABEL"
        NUM_ROW="1"
        SHOW_IF="DB_VERSION == 'SYBSEIQ_15'"
        >
        <DEFAULT>(To use this component, you need first to add jodbc.jar in the Modules view)</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:SYBASE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="20"
      SHOW_IF="DB_VERSION == 'SYBSEIQ_12'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tSybaseConnection"
      NUM_ROW="20"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"
    />

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="2"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>SybaseASE</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="23"
         SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
        <ITEMS DEFAULT="SYBSEIQ_12">
            <ITEM NAME="SYBSEIQ_12" VALUE="jconn3.jar" />
            <ITEM NAME="SYBSEIQ_15" VALUE="jodbc.jar" />
        </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="DATA_SOURCE" FIELD="CLOSED_LIST" NUM_ROW="25"
        REPOSITORY_VALUE="DATA_SOURCE" SHOW_IF="(DB_VERSION == 'SYBSEIQ_15')">
        <ITEMS DEFAULT="DATA_SOURCE_NAME">
            <ITEM NAME="DATA_SOURCE_NAME" VALUE="DSN" />
            <ITEM NAME="FILE_DATA_SOURCE_NAME" VALUE="FILEDSN" />
        </ITEMS>
    </PARAMETER>
    <PARAMETER
      NAME="DATA_SOURCE_NAME"
      FIELD="TEXT"
      NUM_ROW="25"
      REPOSITORY_VALUE="DATA_SOURCE_NAME"
      REQUIRED="true"
      SHOW_IF="(DATA_SOURCE == 'DATA_SOURCE_NAME') AND (DB_VERSION == 'SYBSEIQ_15')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
       <PARAMETER NAME="FILE_DATA_SOURCE_NAME" FIELD="FILE"
       NUM_ROW="25" REQUIRED="true"
       REPOSITORY_VALUE="FILE_DATA_SOURCE_NAME"
       SHOW_IF="(DATA_SOURCE == 'FILE_DATA_SOURCE_NAME') AND (DB_VERSION == 'SYBSEIQ_15')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION != 'SYBSEIQ_15')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION != 'SYBSEIQ_15')"
    >
      <DEFAULT>"2638"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SID"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DB_SCHEMA"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SCHEMA"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="50"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>


    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="60"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="61"
      NOT_SHOW_IF="PARALLELIZE == 'true'"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
        <ITEM NAME="TRUNCATE" VALUE="TRUNCATE"/>
      </ITEMS>
    </PARAMETER>


    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="70"
      REQUIRED="true"
      SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'false'"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/sybaseIQ_bulk.txt"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="80"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="90"
      SHOW="false"
    >
      <DEFAULT>sybase_id</DEFAULT>
    </PARAMETER>

    <!-- SybaseIQLocation Config -->
    <PARAMETER
        NAME="USE_INSERT_LOCATION_SYNTAX"
        FIELD="CHECK"
        NUM_ROW="155"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
        NAME="SOURCE_SERVER_NAME"
        FIELD="TEXT"
        NUM_ROW="175"
        GROUP="SOURCE_CONFIG"
        SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'true'"
    >
        <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
        NAME="SOURCE_DATABASE"
        FIELD="TEXT"
        NUM_ROW="175"
        GROUP="SOURCE_CONFIG"
        SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'true'"
    >
        <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
        NAME="SOURCE_TABLE_SCHEMA"
        FIELD="TEXT"
        NUM_ROW="175"
        GROUP="SOURCE_CONFIG"
        SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'true'"
    >
        <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
        NAME="SOURCE_TABLE"
        FIELD="TEXT"
        NUM_ROW="175"
        GROUP="SOURCE_CONFIG"
        SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'true'"
    >
        <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="USE_FILTER"
       FIELD="CHECK"
       NUM_ROW="185"
       GROUP="SOURCE_CONFIG"
       SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'true'"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="WHERE_CLAUSE"
       FIELD="TEXT"
       NUM_ROW="185"
       GROUP="SOURCE_CONFIG"
       SHOW_IF="(USE_INSERT_LOCATION_SYNTAX == 'true') AND (USE_FILTER == 'true')"
    >
        <DEFAULT>"where "</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="MAPPING_INPUT"
       FIELD="TABLE"
       NUM_ROW="190"
       NB_LINES="6"
       GROUP="SOURCE_CONFIG"
       SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'true'"
    >
        <ITEMS BASED_ON_SCHEMA="true">
            <ITEM NAME="PARAMETER_NAME" FIELD="String" VALUE='""'/>
        </ITEMS>
    </PARAMETER>
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   <PARAMETER
        NAME="PROPERTIES"
        FIELD="TEXT"
        NUM_ROW="10"
        REPOSITORY_VALUE="PROPERTIES_STRING"
        SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
   >
        <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LINES_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="70"
      REQUIRED="true"
      SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'false'"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDS_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="70"
      REQUIRED="true"
      SHOW_IF="(FIXED_LENGTH=='false') AND (USE_INSERT_LOCATION_SYNTAX == 'false')"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCLOSED"
      FIELD="CHECK"
      NUM_ROW="70"
      REQUIRED="true"
      SHOW_IF="(FIXED_LENGTH=='false') AND (USE_INSERT_LOCATION_SYNTAX == 'false')"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
        NAME="FIXED_LENGTH"
        FIELD="CHECK"
        NUM_ROW="80"
        REQUIRED="false"
        SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'false'"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="INCLUDEHEADER" FIELD="CHECK" NUM_ROW="90"
        SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'false'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
       NAME="BLANK_AS_NULL"
       FIELD="CHECK"
       NUM_ROW="100"
       SHOW_IF="(FIXED_LENGTH=='false') AND (USE_INSERT_LOCATION_SYNTAX == 'false')"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="LOCATION_OPTIONS"
       FIELD="CHECK"
       NUM_ROW="110"
       SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'true'"
       GROUP="GROUP_LOCATION_OPTIONS"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="ENCRYPTED_PASSWORD"
       FIELD="CHECK"
       NUM_ROW="112"
       GROUP="GROUP_LOCATION_OPTIONS"
       SHOW_IF="(LOCATION_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="USE_TDS_PACKET_SIZE"
       FIELD="CHECK"
       NUM_ROW="115"
       GROUP="GROUP_LOCATION_OPTIONS"
       SHOW_IF="(LOCATION_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="PACKET_SIZE"
       FIELD="TEXT"
       NUM_ROW="115"
       GROUP="GROUP_LOCATION_OPTIONS"
       SHOW_IF="(USE_TDS_PACKET_SIZE == 'true') AND (LOCATION_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT>512</DEFAULT>
    </PARAMETER>


    <PARAMETER
       NAME="QUOTED_IDENTIFIER"
       FIELD="CHECK"
       NUM_ROW="116"
       GROUP="GROUP_LOCATION_OPTIONS"
       SHOW_IF="(LOCATION_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="QUOTED_IDENTIFIER_STATUS"
       FIELD="CLOSED_LIST"
       NUM_ROW="116"
       GROUP="GROUP_LOCATION_OPTIONS"
       SHOW_IF="(LOCATION_OPTIONS == 'true') AND (QUOTED_IDENTIFIER == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
       <ITEMS DEFAULT="ON">
            <ITEM NAME="ON" VALUE="on" />
            <ITEM NAME="OFF" VALUE="off" />
        </ITEMS>
    </PARAMETER>


    <PARAMETER
       NAME="SET_ISOLATION_LEVEL"
       FIELD="CHECK"
       NUM_ROW="117"
       GROUP="GROUP_LOCATION_OPTIONS"
       SHOW_IF="(LOCATION_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="ISOLATION_LEVEL"
       FIELD="CLOSED_LIST"
       NUM_ROW="117"
       GROUP="GROUP_LOCATION_OPTIONS"
       SHOW_IF="(SET_ISOLATION_LEVEL == 'true') AND (LOCATION_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
       <ITEMS DEFAULT="READ_COMMITTED">
            <ITEM NAME="READ_UNCOMMITTED" VALUE="read uncommitted" />
            <ITEM NAME="READ_COMMITTED" VALUE="read committed" />
            <ITEM NAME="SERIALIZABLE" VALUE="serializable" />
        </ITEMS>
    </PARAMETER>

    <PARAMETER
       NAME="INSERT_LOAD_OPTIONS"
       FIELD="CHECK"
       NUM_ROW="119"
       SHOW_IF="(USE_INSERT_LOCATION_SYNTAX == 'true')"
       GROUP="GROUP_INSERT_LOAD_OPTIONS"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="LIMIT"
       FIELD="TEXT"
       NUM_ROW="120"
       GROUP="GROUP_INSERT_LOAD_OPTIONS"
       SHOW_IF="(INSERT_LOAD_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT></DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="NOTIFY"
       FIELD="TEXT"
       NUM_ROW="120"
       GROUP="GROUP_INSERT_LOAD_OPTIONS"
       SHOW_IF="(INSERT_LOAD_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT></DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="SKIP"
       FIELD="TEXT"
       NUM_ROW="120"
       GROUP="GROUP_INSERT_LOAD_OPTIONS"
       SHOW_IF="(INSERT_LOAD_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT></DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="START_ROW_ID"
       FIELD="TEXT"
       NUM_ROW="120"
       GROUP="GROUP_INSERT_LOAD_OPTIONS"
       SHOW_IF="(INSERT_LOAD_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT></DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="INSERT_SELECT_LOAD_OPTIONS"
       FIELD="CHECK"
       NUM_ROW="125"
       SHOW_IF="USE_INSERT_LOCATION_SYNTAX == 'true'"
       GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="WORD_SKIP_NUMBER"
       FIELD="TEXT"
       NUM_ROW="130"
       GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
       SHOW_IF="(INSERT_SELECT_LOAD_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <DEFAULT></DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="IGNORE_CONSTRAINT"
       FIELD="CHECK"
       NUM_ROW="135"
       SHOW_IF="(INSERT_SELECT_LOAD_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
       GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
        NAME="IGNORE_CONSTRAINT_TABLE"
        FIELD="TABLE"
        NUM_ROW="140"
        NB_LINES="4"
        GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
        SHOW_IF="(IGNORE_CONSTRAINT == 'true') AND (INSERT_SELECT_LOAD_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
        <ITEMS >
          <ITEM NAME="CONSTRAINT_TYPE" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="NULL">
                <ITEM NAME="CHECK" VALUE="check" />
                <ITEM NAME="UNIQUE" VALUE="unique" />
                <ITEM NAME="NULL" VALUE="null" />
                <ITEM NAME="FOREIGN_KEY" VALUE="foreign key" />
                <ITEM NAME="DATA_VALUE" VALUE="data value" />
                <ITEM NAME="ALL" VALUE="all" />
          </ITEMS>
          </ITEM>
          <ITEM NAME="IGNORE_CONSTRAINT_NUMBER" FIELD="TEXT" />
        </ITEMS>
    </PARAMETER>

    <PARAMETER
       NAME="LOG_IN_FILE"
       FIELD="CHECK"
       NUM_ROW="145"
       SHOW_IF="(INSERT_SELECT_LOAD_OPTIONS == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
       GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="MESSAGE_LOG"
       FIELD="TEXT"
       NUM_ROW="148"
       SHOW_IF="(INSERT_SELECT_LOAD_OPTIONS == 'true') AND (LOG_IN_FILE == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
       GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
    >
        <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="ROW_LOG"
       FIELD="TEXT"
       NUM_ROW="148"
       SHOW_IF="(INSERT_SELECT_LOAD_OPTIONS == 'true') AND (LOG_IN_FILE == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
       GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
    >
        <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="LOG_DELIMITED_BY"
       FIELD="TEXT"
       NUM_ROW="148"
       SHOW_IF="(INSERT_SELECT_LOAD_OPTIONS == 'true') AND (LOG_IN_FILE == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
       GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
    >
        <DEFAULT>";"</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="ONLY_LOG"
       FIELD="CHECK"
       NUM_ROW="158"
       SHOW_IF="(INSERT_SELECT_LOAD_OPTIONS == 'true') AND (LOG_IN_FILE == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
       GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
       NAME="ONLY_LOG_TYPE"
       FIELD="CLOSED_LIST"
       NUM_ROW="158"
       GROUP="GROUP_INSERT_SELECT_LOAD_OPTIONS"
       SHOW_IF="(INSERT_SELECT_LOAD_OPTIONS == 'true') AND (LOG_IN_FILE == 'true') AND (ONLY_LOG == 'true') AND (USE_INSERT_LOCATION_SYNTAX == 'true')"
    >
       <ITEMS DEFAULT="UNIQUE">
            <ITEM NAME="CHECK" VALUE="check" />
            <ITEM NAME="UNIQUE" VALUE="unique" />
            <ITEM NAME="NULL" VALUE="null" />
            <ITEM NAME="FOREIGN_KEY" VALUE="foreign key" />
            <ITEM NAME="DATA_VALUE" VALUE="data value" />
            <ITEM NAME="ALL" VALUE="all" />
            <ITEM NAME="WORD" VALUE="word" />
        </ITEMS>
    </PARAMETER>
    
   </ADVANCED_PARAMETERS>

   <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="Driver-Sybase3" MODULE="jconn3.jar" MVN="mvn:org.talend.libraries/jconn3/6.0.0"  REQUIRED="true" />
       <IMPORT NAME="Driver-Sybase" MODULE="jodbc.jar" MVN="mvn:org.talend.libraries/jodbc/6.0.0"  REQUIRED="false" />
    </IMPORTS>
  </CODEGENERATION>

   <RETURNS />
</COMPONENT>
